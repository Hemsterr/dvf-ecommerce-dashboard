{
  "name": "dvf-ecommerce-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apisauce": "^1.0.2",
    "flexboxgrid": "^6.3.1",
    "lodash.isequal": "^4.5.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy-dev": "sh -ac '. .env.development; make deploy-dev'",
    "deploy-prod": "sh -ac '. .env.production; make deploy-prod'",
    "test": "jest --config=jest.config.js",
    "test:coverage": "yarn test --coverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "prettier": "prettier --write {src,__tests__,.stories}/**/*.{js,jsx}",
    "eslint": "eslint {src,__tests__,.stories}/**/*.{js,jsx}",
    "flow": "flow"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-info": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@testing-library/react": "^9.3.1",
    "babel-cli": "^6.26.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "dotenv": "^8.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "flow-bin": "^0.111.0",
    "flow-typed": "^2.6.2",
    "husky": "^3.0.9",
    "imagemin-lint-staged": "^0.4.0",
    "lint-staged": "^9.4.2",
    "node-sass": "^4.13.0",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn flow && yarn test"
    }
  },
  "lint-staged": {
    "{src/**/*.js": [
      "prettier --write",
      "git add"
    ],
    "{src,__tests__,stories}/**/*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  }
}
